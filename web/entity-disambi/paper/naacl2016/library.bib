@inproceedings{Ritter2011a,
abstract = {People tweet more than 100 Million times daily, yielding a noisy, informal, but some- times informative corpus of 140-character messages that mirrors the zeitgeist in an un- precedented manner. The performance of standard NLP tools is severely degraded on tweets. This paper addresses this issue by re-building the NLP pipeline beginning with part-of-speech tagging, through chunking, to named-entity recognition. Our novel T-NER system doubles F1 score compared with the Stanford NER system. T-NER leverages the redundancy inherent in tweets to achieve this performance, using LabeledLDA to exploit Freebase dictionaries as a source of distant supervision. LabeledLDA outperforms co- training, increasing F1 by 25{\%} over ten com- mon entity types.},
author = {Ritter, Alan and Clark, Sam and Etzioni, Oren},
booktitle = {EMNLP '11},
pages = {1524--1534},
title = {{Named Entity Recognition in Tweets : An Experimental Study}},
year = {2011}
}
